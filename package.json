{
  "name": "react-universal-boilerplate",
  "version": "1.0.0",
  "description": "react babel webpack ssr hmr redux react-router",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development env-cmd .env babel-node scripts/start",
    "serve": "cross-env NODE_ENV=production env-cmd .env babel-node scripts/serve",
    "build": "webpack --config config/webpack.prod.js",
    "serve:https": "cross-env NODE_ENV=production env-cmd .env babel-node scripts/serve-https.js",
    "test": "jest -u --json --outputFile=.storybook/results.jest.json --watch",
    "analyze": "node scripts/analyze-bundles.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./scripts/test-setup.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint . --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ungerpeter/react-universal-boilerplate.git"
  },
  "author": "Peter Unger <peps.unger@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ungerpeter/react-universal-boilerplate/issues"
  },
  "homepage": "https://github.com/ungerpeter/react-universal-boilerplate#readme",
